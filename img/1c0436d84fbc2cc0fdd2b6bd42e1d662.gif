    <script type="text/javascript">
        
    var GetUrlValue = function(name) {
        var reg = new RegExp('(^|&)' + name + '=([^&]*)(&|$)', 'i');
        var r = window.location.search.substr(1).match(reg);
        if (r != null) {
            try {
                return decodeURIComponent(r[2]);
            } catch (e) {
                return null;
            }
        }
        return null;
    }
    if (GetUrlValue('bvid')) {
      window.show_bv = 1;
    } else {
      window.show_bv = 0;
    }
    </script>
    <style type="text/css">
    html, body, .player {
        margin: 0;
        padding: 0;
        width: 100%;
        height: 100%;
        overflow: hidden;
    }
    .bilibili-player-video-panel-image-detail {
        display: none!important;
    }
    </style>
</head>
<body>
    <div id="heimu"></div>
    <div class="player">
      <div id="bofqi"></div>
    </div>
    <script type="text/javascript">
       if (!!navigator.userAgent.match(/AppleWebKit.*Mobile.*/) && !navigator.userAgent.match(/Domono/)) {
                window.location.href = '//www.bilibili.com/blackboard/webplayer/mbplayer.html' + window.location.search;
        }
        function get_cookie(Name) {
            var search = Name + "="//查询检索的值
            var returnvalue = "";//返回值
            if (document.cookie.length > 0) {
                sd = document.cookie.indexOf(search);
                if (sd!= -1) {
                    sd += search.length;
                    end = document.cookie.indexOf(";", sd);
                    if (end == -1)
                    end = document.cookie.length;
                    //unescape() 函数可对通过 escape() 编码的字符串进行解码。
                    returnvalue=unescape(document.cookie.substring(sd, end))
                }
            } 
            return returnvalue;
        }

        async function isHit(){
            const varList = "embed_other";
            let buvid = get_cookie("CURRENT_PID");

            return await $.get(`https://api.bilibili.com/x/web-frontend/ab/getabtestinfo?varlist=${varList}&mid=0&buvid=${buvid}`)
        }
        let labPromise = isHit();

        labPromise.then(async function(data){
            if (data.result&&data.result.length){
                window.location.href = '//www.bilibili.com/blackboard/webplayer/embed-whitelist-other.html' + window.location.search;
            }else{
                window.location.href = '//www.bilibili.com/blackboard/webplayer/embed-old.html' + window.location.search;
            }
        }).catch(function(){
            window.location.href = '//www.bilibili.com/blackboard/webplayer/embed-old.html' + window.location.search;
        })
    </script>
  </body>
</html>